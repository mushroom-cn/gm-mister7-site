    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^3.0.0",
    "css-loader": "^6.7.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-config-xo": "^0.43.1",
