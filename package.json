  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "test": "jest",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "cross-env NODE_OPTIONS=--max-old-space-size=8192 webpack serve",
    "scan": "babel -f babel.config.json 'src/**/*.{js,jsx,ts,tsx}'",
    "swagger:json": "curl -o ./src/plugins/VideoList/api/swagger.json http://localhost:3000/api-json",
    "swagger:api": "swagger-typescript-api -p ./src/plugins/VideoList/api/swagger.json -o ./src/plugins/VideoList/api/ -n api.ts --axios --sort-types --extract-enums  --add-readonly  --single-http-client --templates ./templates",
    "swagger:create": "node swagger-api.js",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
